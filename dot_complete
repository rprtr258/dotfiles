_mani_init()
{
    last_command="mani_init"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--auto-discovery")
    local_nonpersistent_flags+=("--auto-discovery")
    flags+=("--vcs=")
    two_word_flags+=("--vcs")
    flags_with_completion+=("--vcs")
    flags_completion+=("__mani_handle_go_custom_completion")
    local_nonpersistent_flags+=("--vcs")
    local_nonpersistent_flags+=("--vcs=")
    flags+=("--config=")
    two_word_flags+=("--config")
    two_word_flags+=("-c")
    flags+=("--no-color")
    flags+=("--user-config=")
    two_word_flags+=("--user-config")
    two_word_flags+=("-u")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

