    flags_completion=()

    flags+=("--dry-run")
    local_nonpersistent_flags+=("--dry-run")
    flags+=("--describe")
    local_nonpersistent_flags+=("--describe")
    flags+=("--list-hosts")
    local_nonpersistent_flags+=("--list-hosts")
    flags+=("--verbose")
    flags+=("-V")
    local_nonpersistent_flags+=("--verbose")
    local_nonpersistent_flags+=("-V")
    flags+=("--strategy=")
    two_word_flags+=("--strategy")
    flags_with_completion+=("--strategy")
    flags_completion+=("__sake_handle_go_custom_completion")
    two_word_flags+=("-S")
    flags_with_completion+=("-S")
    flags_completion+=("__sake_handle_go_custom_completion")
    local_nonpersistent_flags+=("--strategy")
    local_nonpersistent_flags+=("--strategy=")
    local_nonpersistent_flags+=("-S")
    flags+=("--forks=")
    two_word_flags+=("--forks")
    two_word_flags+=("-f")
    local_nonpersistent_flags+=("--forks")
    local_nonpersistent_flags+=("--forks=")
    local_nonpersistent_flags+=("-f")
    flags+=("--batch=")
    two_word_flags+=("--batch")
    two_word_flags+=("-b")
