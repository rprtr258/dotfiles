    two_word_flags+=("-c")
    flags+=("--no-color")
    flags+=("--user-config=")
    two_word_flags+=("--user-config")
    two_word_flags+=("-u")

    must_have_one_flag=()
    must_have_one_noun=()
    must_have_one_noun+=("bash")
